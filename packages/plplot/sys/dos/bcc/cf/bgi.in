PLLIB = plplotl.lib
.c.obj:
	$(CC) -c $(CFLAGS) $*.c
.rc.res:
	$(BRC) -r $(BRCFLAGS) $*.rc
.obj.exe:
	tlink @&&|
/v c0l $*.obj
$*,$*
plplotl.lib graphics.lib mathl.lib fp87.lib cl.lib
|

libs:   libc
libc:   plplotl.lib

$(DRV_OBJ):     pldevs.h
plcore.obj:     pldevs.h

plplotl.lib: $(OBJ) $(DRV_OBJ)
	@echo Ready to build plplotl.lib
	-@del plplotl.lib
	tlib @&&!
plplotl\
+$(OBJ:.obj=.obj +)\
$(DRV_OBJ:.obj=.obj +)\
,plplotl.lst
!

plrender: plrender.exe plplotl.lib
