environment
  tabular_data
    tabular_data_file 'incremental_sampling.dat'
  results_output
    results_output_file 'incremental_sampling'
    hdf5
      interface_selection none

method
  id_method 'sampling'
  sampling
    seed 1337
    samples 20
    refinement_samples 20 40
    probability_levels 0.25 0.5 0.75
                       0.3  0.4 0.7
    response_levels 0.6 0.2
    std_regression_coeffs
  output verbose

variables
  uniform_uncertain 2
    descriptors 'x1' 'x2'
    lower_bounds 0.0 0.0
    upper_bounds 1.0 1.0
  continuous_state 1
    descriptors 'x3'

responses
  id_responses 'sim_r'
  response_functions 2
   descriptors 'f' 'c'
   no_gradients no_hessians

interface
  direct
   analysis_drivers 'text_book'

   
  
