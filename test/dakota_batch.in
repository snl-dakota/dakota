#@ s*: Label=FastTest

environment
  tabular_data

method
  sampling seed 1234 samples 100

variables
  uniform_uncertain 1
   descriptors 'x1' 
  lower_bounds -1.0 
  upper_bounds 2.0  

responses 
  response_functions 1
  no_gradients
  no_hessians

interface
  # Using a system interface so Windows will use python interpreter for the driver
  system 
  batch size 10
   analysis_drivers '@Python_EXECUTABLE@ dakota_batch.py'
  parameters_file 'params.in'
  results_file 'results.out'
  file_save
