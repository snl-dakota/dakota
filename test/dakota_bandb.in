#@ s*: Label=FastTest
#@ s1: DakotaConfig=HAVE_NPSOL
#@ s2: DakotaConfig=HAVE_ACRO

environment
  method_pointer = 'BandB'

method
  id_method = 'BandB'
  branch_and_bound
    output verbose
    method_pointer = 'SubNLP'

method
  id_method = 'SubNLP'
  conmin_frcg					#s0
#  npsol_sqp					#s1
#  coliny_ea					#s2
#    seed = 12345				#s2
    max_iterations = 100
    max_function_evaluations = 100

variables,
	continuous_design = 3
	  initial_point   -1.0    1.5   2.0
	  upper_bounds    10.0   10.0  10.0
	  lower_bounds   -10.0  -10.0 -10.0
	  descriptors      'x1'  'x2'  'x3'
#	discrete_design_range = 2		#s2
#	  initial_point      2     2		#s2
#	  lower_bounds       1     1		#s2
#	  upper_bounds       4     9		#s2
#	  descriptors      'y1'   'y2'		#s2
	discrete_design_set			#s0,#s1
	  integer = 1				#s0,#s1
	    num_set_values = 4			#s0,#s1
	    set_values = 1 2 3 4		#s0,#s1
	    descriptors  'z1'			#s0,#s1
	  categorical 'no'			#s0,#s1

interface,
	direct
	  analysis_driver = 'text_book'

responses,
	objective_functions = 1
	nonlinear_inequality_constraints = 2
	numerical_gradients
	no_hessians
