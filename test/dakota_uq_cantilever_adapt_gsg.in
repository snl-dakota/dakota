environment,
#	output_precision = 16

method,
	polynomial_chaos				#s0,#s1,#s2,#s3
#	stoch_collocation				#s4,#s5,#s6,#s7,#s8
	  dimension_adaptive p_refinement generalized
#	    max_refinement_iterations  = 10
