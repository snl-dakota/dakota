#	  num_response_levels = 0 1 1			#s2,#s3,#s6,#s7,#s8
#	  response_levels = 0.0 0.0			#s2,#s3,#s6,#s7,#s8
#	  compute reliabilities				#s2,#s3,#s6,#s7,#s8
#	  compute probabilities
