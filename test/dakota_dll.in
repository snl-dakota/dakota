#@ s*: Label=FastTest
#@ s0: ExecCmd='dll_tester' ExecArgs='1' Restart=none CheckOutput='dakota_dll_tester.log'

# Dakota DLL test, based on rosenbrock users test s0
# To execute test outside of the Dakota testing infrastructure:
#    dll_tester 1 dakota_dll.in

environment
  tabular_data
    tabular_data_file = 'rosen_multidim.dat' 

method
  multidim_parameter_study     
    partitions = 8 8           

model
  single

variables
  continuous_design = 2                
    lower_bounds     -2.0     -2.0     
    upper_bounds      2.0      2.0     
    descriptors       'x1'     "x2"    

interface
  analysis_driver = 'plugin_rosenbrock'
    direct          

responses
  response_functions = 1    
  no_gradients        
  no_hessians
