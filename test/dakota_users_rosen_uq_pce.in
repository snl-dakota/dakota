
strategy,
	single_method #graphics

method,
	polynomial_chaos
	  quadrature_order   = 5	
	  dimension_preference = 5 3	
	  samples = 10000		
	  seed = 12347 rng rnum2	
	  response_levels =		
	  .1 1. 50. 100. 500. 1000.	
	  variance_based_decomp #univariate_effects

variables,
	uniform_uncertain = 2			
      	  lower_bounds      = -2.  -2. 		
	  upper_bounds      =  2.   2.		
	  descriptors       = 'x1' 'x2'		

interface,
	direct
	analysis_driver = 'rosenbrock'		

responses,
	response_functions = 1
	no_gradients
	no_hessians

