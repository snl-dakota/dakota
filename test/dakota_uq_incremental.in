#@ s0: Restart=write
#@ s1: Restart=read

# dakota_incremental_sampling.in
# Restart specification for tests (test 0: write, test 1: read)
# 0=Rw, 1=Rr

environment,
	  tabular_graphics_data

method,
	sampling
          seed = 234
	  sample_type lhs samples = 10		#s0
#         sample_type incremental_lhs		#s1
#	  previous_samples = 10 samples = 20	#s1

variables,
	uniform_uncertain = 2
	  lower_bounds = 2 2
	  upper_bounds = 3 3
	normal_uncertain = 2
	  means = 10 2
          std_deviations = 2. 1.0

interface,
	system
	  analysis_driver = 'text_book'
	  parameters_file = 'params.in'
	  results_file = 'results.out'

responses,
	response_functions = 2
	no_gradients
	no_hessians
