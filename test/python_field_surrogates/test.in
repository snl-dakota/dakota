
environment
        tabular_data
          tabular_data_file = "surr_tabular.dat"
        method_pointer = 'UQ'

method,
        id_method = 'UQ'
        model_pointer = 'SURR'
        sampling,
        sample_type lhs
          samples = 25
          seed = 98765
          #response_levels = 0.1 1.0 10.0 100.0
          #distribution cumulative
        output verbose

model,
        id_model = 'SURR'
        surrogate global,
          dace_method_pointer = 'DACE'
          reuse_samples all
#          polynomial linear
          experimental_python
            class_path_and_name = "tester.Surrogate"

method,
        id_method = 'DACE'
        model_pointer = 'DACE_M'
        sampling
          sample_type lhs
          samples = 10
          seed = 5034
model,
        id_model = 'DACE_M'
        single
          interface_pointer = 'I1'

variables,
        uniform_uncertain = 2
          lower_bounds = -2.0   -2.0
          upper_bounds =  2.0    2.0
          descriptors  = 'x1' 'x2'

interface,
        python
          analysis_driver = 'tester:funcs'
        id_interface = 'I1'

responses,
        response_functions = 3
        scalar_responses = 1
        field_responses = 2
        lengths = 2 3
        no_gradients
        no_hessians

