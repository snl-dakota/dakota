#@ *: DakotaConfig=HAVE_DOT
# Dakota Input File: textbook_pareto_strat.in
environment
  top_method_pointer = 'PS'
  graphics

method
  id_method = 'PS'
  pareto_set
    opt_method_pointer = 'NLP'
    multi_objective_weight_sets =
        1.   0.   0.
        0.   1.   0.
        0.   0.   1.
        .333 .333 .333

method
  id_method = 'NLP'
  dot_bfgs

model
  single

variables
  continuous_design = 2
    initial_point     0.9    1.1
    upper_bounds      5.8    2.9
    lower_bounds      0.5   -2.9
    descriptors       'x1'   'x2'

interface
  fork #asynchronous
     analysis_driver = 'text_book'

responses
  objective_functions = 3
   analytic_gradients
  no_hessians
