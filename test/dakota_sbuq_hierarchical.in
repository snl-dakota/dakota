model,
	id_model = 'LOFI'
	single
	  interface_pointer = 'LOFI_FN'

interface,
	id_interface = 'LOFI_FN'
	direct					#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s10,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19,#s20,#s22,#s23,#s24,#s25,#s26
